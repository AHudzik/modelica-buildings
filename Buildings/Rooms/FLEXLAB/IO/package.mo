within Buildings.Rooms.FLEXLAB;
package IO "Package of example models for communication with CalBay"
extends Modelica.Icons.Package;




  annotation(Documentation(info="<html>
  <p>
  This package contains sample models demonstrating communication with 
  the CalBay adapter to control the lights in a test cell. All models
  in this package require use of a Python script for the communication
  with the CalBay adapter. Modifications to the script will be required.
  See the comments in the script for more information on how to edit the
  script.
  </p>
  </html>"));

end IO;
